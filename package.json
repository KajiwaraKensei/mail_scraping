{
  "name": "mail_scraping",
  "version": "1.0.0",
  "description": " scrapingテスト",
  "main": "src/main.ts",
  "scripts": {
    "build": "webpack",
    "watch": "webpack -w",
    "dev": "ts-node-dev --respawn src/main.ts",
    "pre_commit": "lint-staged",
    "prepare": "husky install",
    "check-types": "tsc --noEmit",
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "fmt": "prettier --write 'src/**/*.{js,ts,json}'",
    "lint": "npm-run-all eslint check-types",
    "lint:fix": "npm-run-all eslint:fix check-types fmt",
    "nibble": "eslint-nibble 'src/**/*.{js,ts,json}'"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,sass,scss}": [
      "stylelint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KajiwaraKensei/mail_scraping.git"
  },
  "author": "KajiwaraKensei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KajiwaraKensei/mail_scraping/issues"
  },
  "homepage": "https://github.com/KajiwaraKensei/mail_scraping#readme",
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "puppeteer": "^10.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-nibble": "^7.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "ts-loader": "^9.2.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
}
